#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;

int main()
{
	float x, y, s;
	cout <<"x=";
	cin >> x;
	cout <<"y=";
	cin >> y;
	cin >> s;
	{ if (((x>0 && y<0) || (x<0 && y>0)) && (x*x+y*y<4))
	    		cout << "Da";
	else {if ((x>0 && y>0) || (x<0 && y<0) || (((x>0 && y<0) || (x<0 && y>0)) && (x*x+y*y>4)))
		cout << "Net";
	else
		cout << "Na granice";
}
}
	return 0;
}
